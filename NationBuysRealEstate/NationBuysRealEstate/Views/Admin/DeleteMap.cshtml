@model NationBuysRealEstate.Map

@{
    ViewBag.Title = "DeleteMap";
    Layout = null;
}

<button type="button" class="btn btn-sm btn-default pull-right" data-dismiss="modal">X</button>
<h2>DeleteMap</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Society_Name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Society_Name)
        </dd>

