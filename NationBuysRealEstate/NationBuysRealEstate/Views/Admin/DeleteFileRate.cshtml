@model NationBuysRealEstate.FileRate

@{
    ViewBag.Title = "DeleteFileRate";
    Layout = null;
}
<button type="button" class="btn btn-sm btn-default pull-right" data-dismiss="modal">X</button>
<h2>DeleteFileRate</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label col-md-2" for="FileRates_Image">FileRates</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FileRates_Image)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions">
            <input type="submit" value="Delete" class="btn btn-info btn-md" />
        </div>
    }
</div>
